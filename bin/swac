#!/usr/bin/env php
<?php

use Yaoi\Cli\Command\Application\Runner;

set_time_limit(20);

foreach (array(__DIR__ . '/../../../autoload.php', __DIR__ . '/../vendor/autoload.php') as $file) {
    if (file_exists($file)) {
        require_once $file;
        break;
    }
}

Runner::create(new \Swac\Command\App())->run();